@helper RedDiv(string icon, string title)
{<div class="col-md-3 col_Tech">
        <div class="col_tech_Red">
            <div class="icon_tech_div">
                <i class="@icon icon_style_tech" aria-hidden="true"></i>
            </div>
            <h4 class="h4_icon_tech">@title</h4>
        </div>
    </div>
}
